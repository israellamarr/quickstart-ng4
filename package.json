{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080 --host 0.0.0.0",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean": "rimraf dist",
    "build_dev": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail"
  },
  "license": "MIT",
